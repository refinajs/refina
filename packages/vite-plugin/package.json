{
  "name": "vite-plugin-refina",
  "version": "0.0.0",
  "description": "The vite plugin for Refina framework.",
  "keywords": [
    "refina",
    "vite-plugin"
  ],
  "files": [
    "dist",
    "plugin.ts",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/plugin.js",
  "types": "./dist/plugin.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      }
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "author": "_Kerman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KermanX/refina"
  },
  "readme": "https://github.com/KermanX/refina#readme",
  "bugs": "https://github.com/KermanX/refina/issues",
  "license": "MIT",
  "dependencies": {
    "@refina/transformer": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@types/node": "^20.6.5",
    "vite": "^4.4.8"
  }
}
